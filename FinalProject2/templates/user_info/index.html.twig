{% extends 'base.html.twig' %}

{% block title %}UserInfo index{% endblock %}

{% block body %}
    <h1>UserInfo index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>RegistredAt</th>
                <th>BornAt</th>
                <th>Status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user_info in user_infos %}
            <tr>
                <td>{{ user_info.id }}</td>
                <td>{{ user_info.registredAt ? user_info.registredAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ user_info.bornAt ? user_info.bornAt|date('Y-m-d') : '' }}</td>
                <td>{{ user_info.Status }}</td>
                <td>
                    <a href="{{ path('app_user_info_show', {'id': user_info.id}) }}">show</a>
                    <a href="{{ path('app_user_info_edit', {'id': user_info.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_user_info_new') }}">Create new</a>
{% endblock %}
